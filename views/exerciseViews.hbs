<div class="container">
    <h2>Exercises list</h2>

    {{#each Exercises}}
    <div class="card mb-3">
        <div class="card-body">
            <h2 class="card-title">{{name}}</h2>
            <p class="card-text">{{weight}}</p>
            <p class="card-text">{{repeatsNumber}}</p>
            <a href="" class="btn btn-primary">More</a>
        </div>
    </div>
    {{/each}}
</div>